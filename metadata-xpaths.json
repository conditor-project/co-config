[
  {
    "name": "title",
    "type": "object",
    "fields": [
      {
        "name": "default",
        "type": "simpleString",
        "path": "normalize-space(//TEI:text/TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:analytic/TEI:title)"
      },
      {
        "name": "fr",
        "type": "simpleString",
        "path": "normalize-space(//TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:analytic/TEI:title[@xml:lang='fr'])"
      },
      {
        "name": "en",
        "type": "simpleString",
        "path": "normalize-space(//TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:analytic/TEI:title[@xml:lang='en'])"
      },
      {
        "name": "monography",
        "type": "simpleString",
        "path": "normalize-space(//TEI:body//TEI:biblFull//TEI:biblStruct//TEI:monogr//TEI:title[@level='m']/text())"
      },
      {
        "name": "journal",
        "type": "simpleString",
        "path": "normalize-space(//TEI:body//TEI:biblFull//TEI:biblStruct//TEI:monogr//TEI:title[@level='j']/text())"
      },
      {
        "name": "meeting",
        "type": "simpleString",
        "path": "normalize-space(//TEI:body//TEI:meeting//TEI:title)"
      }
    ]
  },
  {
    "name": "first3AuthorNames",
    "type": "bloc",
    "path": "//TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:author[@role='aut' or @role='crp']//TEI:persName",
    "separator": " ",
    "limit": 3,
    "concat": true,
    "fields": {
      "type": "array",
      "concat": true,
      "separator": " ",
      "fields": [
        {
          "name": "surname",
          "type": "simpleString",
          "path": "string(//TEI:surname)"
        },
        {
          "name": "forename",
          "type": "simpleString",
          "path": "string(//TEI:forename)"
        }
      ]
    }
  },
  {
    "name": "authorNames",
    "type": "bloc",
    "path": "//TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:author[@role='aut' or @role='crp']/TEI:persName",
    "separator": " ",
    "concat": true,
    "fields": {
      "type": "array",
      "concat": true,
      "separator": " ",
      "fields": [
        {
          "name": "surname",
          "type": "simpleString",
          "path": "string(//TEI:surname)"
        },
        {
          "name": "forename",
          "type": "simpleString",
          "path": "string(//TEI:forename)"
        }
      ]
    }
  },
  {
    "name": "first3AuthorNamesWithInitials",
    "type": "bloc",
    "path": "//TEI:body//TEI:biblFull//TEI:sourceDesc//TEI:author[@role='aut' or @role='crp']//TEI:persName",
    "separator": " ",
    "limit": 3,
    "concat": true,
    "fields": {
      "separator": " ",
      "type": "array",
      "concat": true,
      "fields": [
        {
          "name": "surname",
          "type": "simpleString",
          "path": "string(//TEI:surname)"
        },
        {
          "name": "forename",
          "path": "string(//TEI:forename)",
          "type": "simpleString",
          "regexp": "(?:\\s*\\b)([a-zA-Z])(?:[^\\s-]*)",
          "flag": "gm",
          "replace": "$1"
        }
      ]
    }
  },
  {
    "name": "authors",
    "type": "bloc",
    "path": "//TEI:body/TEI:listBibl/TEI:biblFull/TEI:sourceDesc//TEI:author[@role='aut' or @role='crp']",
    "fields": {
      "type": "struct",
      "fields": [
        {
          "name": "surname",
          "type": "simpleString",
          "path": "string(//TEI:persName//TEI:surname)"
        },
        {
          "name": "forename",
          "type": "simpleString",
          "path": "string(//TEI:persName//TEI:forename)"
        },
        {
          "name": "halAuthorId",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='halauthorid'])"
        },
        {
          "name": "idHal",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='idhal' and @notation='numeric'])"
        },
        {
          "name": "researcherId",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='researcherid'])"
        },
        {
          "type": "bloc",
          "name": "affiliations",
          "path": "//TEI:affiliation",
          "fields": {
            "type": "struct",
            "fields": [
              {
                "type": "simpleString",
                "name": "ref",
                "path": "first-of-split(concat(normalize-space(//@ref),';',normalize-space(//@n)),';')"
              },
              {
                "type": "simpleString",
                "name": "address",
                "path": "normalize-space(//text())"
              },
              {
                "type": "bloc",
                "name": "isni",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='isni'])",
                  "allowEmpty": false
                }
              },
              {
                "type": "bloc",
                "name": "idRef",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='idRef'])",
                  "allowEmpty": false
                }
              },
              {
                "type": "bloc",
                "name": "rnsr",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='rnsr'])",
                  "allowEmpty": false
                }
              }
            ]
          }
        },
        {
          "type": "bloc",
          "name": "idRef",
          "path": "child::*/TEI:idno[lower-case(@type)='idref']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "type": "bloc",
          "name": "isni",
          "path": "child::*/TEI:idno[lower-case(@type)='isni']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "type": "bloc",
          "name": "orcId",
          "path": "child::*/TEI:idno[lower-case(@type)='orcid']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "type": "bloc",
          "name": "viaf",
          "path": "child::*/TEI:idno[lower-case(@type)='viaf']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        }
      ]
    }
  },
