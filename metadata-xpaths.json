              {
                "type": "simpleString",
                "name": "address",
                "path": "normalize-space(//text())"
              },
              {
                "type": "bloc",
                "name": "idRef",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='idRef'])",
                  "allowEmpty": false
                }
              },
              {
                "type": "bloc",
                "name": "isni",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='isni'])",
                  "allowEmpty": false
                }
              },
              {
                "type": "simpleString",
                "name": "ref",
                "path": "first-of-split(concat(normalize-space(//@ref),';',normalize-space(//@n)),';')"
              },
              {
                "type": "bloc",
                "name": "rnsr",
                "path": "//TEI:org",
                "fields": {
                  "type": "simpleString",
                  "path": "normalize-space(child::*/TEI:idno[@type='rnsr'])",
                  "allowEmpty": false
                }
              }
            ]
          }
        },
        {
          "name": "forename",
          "type": "simpleString",
          "path": "string(//TEI:persName//TEI:forename)"
        },
        {
          "name": "fullname",
          "type": "simpleString",
          "path": "concat(//TEI:persName//TEI:forename,' ',//TEI:persName//TEI:surname)"
        },
        {
          "name": "halAuthorId",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='halauthorid'])"
        },
        {
          "name": "idHal",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='idhal' and @notation='numeric'])"
        },
        {
          "type": "bloc",
          "name": "idRef",
          "path": "child::*/TEI:idno[lower-case(@type)='idref']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "type": "bloc",
          "name": "isni",
          "path": "child::*/TEI:idno[lower-case(@type)='isni']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "type": "bloc",
          "name": "orcId",
          "path": "child::*/TEI:idno[lower-case(@type)='orcid']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        },
        {
          "name": "researcherId",
          "type": "simpleString",
          "path": "string(//TEI:idno[lower-case(@type)='researcherid'])"
        },
        {
          "name": "surname",
          "type": "simpleString",
          "path": "string(//TEI:persName//TEI:surname)"
        },
        {
          "type": "bloc",
          "name": "viaf",
          "path": "child::*/TEI:idno[lower-case(@type)='viaf']",
          "fields": {
            "type": "simpleString",
            "path": "normalize-space(.)"
          }
        }
      ]
    }
  },
